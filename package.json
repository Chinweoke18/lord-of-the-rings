{
    "name": "chinweoke-lord-of-the-ring-sdk",
    "version": "1.0.0",
    "description": "This is an SDK for the Lord of the Rings",
    "main": "dist/index.js",
    "types": "dist/index.d.ts",
    "files": [
        "dist/**/*",
        "LICENSE",
        "package.json",
        "design.md",
        "README.md"
    ],
    "scripts": {
        "test": "echo \"Error: no test specified\" && exit 1",
